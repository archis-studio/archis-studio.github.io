<svg xmlns="http://www.w3.org/2000/svg" width="400" height="500" viewBox="0 0 400 500">
  <defs>
    <linearGradient id="dataGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#b19cd9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#9b7bc7;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="500" fill="url(#dataGradient)"/>
  
  <!-- Data visualization elements -->
  <g transform="translate(50, 100)">
    <!-- Beautiful chart -->
    <path d="M0,300 Q75,250 150,200 T300,120" stroke="white" stroke-width="4" fill="none" opacity="0.9"/>
    <path d="M0,320 Q75,280 150,240 T300,160" stroke="white" stroke-width="2" fill="none" opacity="0.6"/>
    
    <!-- Data points -->
    <circle cx="0" cy="300" r="6" fill="white"/>
    <circle cx="75" cy="250" r="6" fill="white"/>
    <circle cx="150" cy="200" r="8" fill="white"/>
    <circle cx="225" cy="160" r="6" fill="white"/>
    <circle cx="300" cy="120" r="8" fill="white"/>
    
    <!-- Grid lines -->
    <g stroke="white" stroke-width="1" opacity="0.2">
      <line x1="0" y1="0" x2="0" y2="350"/>
      <line x1="75" y1="0" x2="75" y2="350"/>
      <line x1="150" y1="0" x2="150" y2="350"/>
      <line x1="225" y1="0" x2="225" y2="350"/>
      <line x1="300" y1="0" x2="300" y2="350"/>
    </g>
  </g>
  
  <!-- Floating data elements -->
  <circle cx="80" cy="50" r="15" fill="white" opacity="0.3"/>
  <text x="80" y="55" text-anchor="middle" fill="#b19cd9" font-family="JetBrains Mono" font-size="10">AI</text>
  
  <circle cx="320" cy="80" r="12" fill="white" opacity="0.3"/>
  <text x="320" y="85" text-anchor="middle" fill="#b19cd9" font-family="JetBrains Mono" font-size="8">ML</text>
  
  <circle cx="350" cy="450" r="18" fill="white" opacity="0.3"/>
  <text x="350" y="456" text-anchor="middle" fill="#b19cd9" font-family="JetBrains Mono" font-size="10">DS</text>
  
  <!-- Title -->
  <g transform="translate(200, 40)" text-anchor="middle" fill="white" font-family="Playfair Display, serif">
    <text y="0" font-size="24" font-weight="500">Data Poetry</text>
  </g>
</svg>