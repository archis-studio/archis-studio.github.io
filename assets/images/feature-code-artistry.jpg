<svg xmlns="http://www.w3.org/2000/svg" width="400" height="500" viewBox="0 0 400 500">
  <defs>
    <linearGradient id="codeGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2d2d2d;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="500" fill="url(#codeGradient)"/>
  
  <!-- Code window frame -->
  <rect x="30" y="80" width="340" height="340" rx="15" fill="#faf7f2" opacity="0.95"/>
  <rect x="30" y="80" width="340" height="40" rx="15" fill="#d4af37" opacity="0.8"/>
  
  <!-- Window buttons -->
  <circle cx="50" cy="100" r="6" fill="#ff6b6b"/>
  <circle cx="70" cy="100" r="6" fill="#ffd93d"/>
  <circle cx="90" cy="100" r="6" fill="#6bcf7f"/>
  
  <!-- Code lines -->
  <g transform="translate(50, 140)" fill="#1a1a1a" opacity="0.8" font-family="JetBrains Mono, monospace" font-size="12">
    <text y="20">const elegantCode = {</text>
    <text y="40">  beauty: true,</text>
    <text y="60">  performance: 'optimal',</text>
    <text y="80">  maintainable: forever</text>
    <text y="100">};</text>
    <text y="140">// Where art meets logic</text>
    <text y="160">function createMasterpiece() {</text>
    <text y="180">  return code.with(passion)</text>
    <text y="200">    .style(elegance)</text>
    <text y="220">    .deploy(confidence);</text>
    <text y="240">}</text>
  </g>
  
  <!-- Artistic elements -->
  <path d="M350,450 Q380,400 370,350" stroke="#d4af37" stroke-width="3" fill="none" opacity="0.6"/>
  <circle cx="320" cy="460" r="8" fill="#e8b4b8" opacity="0.7"/>
  
  <!-- Title area -->
  <g transform="translate(200, 50)" text-anchor="middle" fill="white" font-family="Playfair Display, serif">
    <text y="0" font-size="24" font-weight="500">Code Artistry</text>
  </g>
</svg>