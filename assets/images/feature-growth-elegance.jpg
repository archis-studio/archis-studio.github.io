<svg xmlns="http://www.w3.org/2000/svg" width="400" height="500" viewBox="0 0 400 500">
  <defs>
    <linearGradient id="growthGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#9caf88;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7d9c6b;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="500" fill="url(#growthGradient)"/>
  
  <!-- Elegant growth curve -->
  <path d="M50,450 Q100,380 150,300 Q200,200 250,150 Q300,100 350,50" 
        stroke="white" stroke-width="6" fill="none" opacity="0.8"/>
  
  <!-- Growth milestones -->
  <g fill="white">
    <circle cx="50" cy="450" r="8" opacity="0.7"/>
    <circle cx="100" cy="380" r="10" opacity="0.8"/>
    <circle cx="150" cy="300" r="12" opacity="0.9"/>
    <circle cx="200" cy="200" r="14" opacity="0.9"/>
    <circle cx="250" cy="150" r="16"/>
    <circle cx="300" cy="100" r="18"/>
    <circle cx="350" cy="50" r="20"/>
  </g>
  
  <!-- Milestone labels -->
  <g fill="white" font-family="Montserrat" font-size="10" text-anchor="middle" opacity="0.8">
    <text x="50" y="470">Start</text>
    <text x="100" y="400">Learn</text>
    <text x="150" y="320">Grow</text>
    <text x="200" y="220">Excel</text>
    <text x="250" y="170">Lead</text>
    <text x="300" y="120">Inspire</text>
    <text x="350" y="70">Transform</text>
  </g>
  
  <!-- Decorative elements -->
  <g transform="translate(30, 30)" fill="white" opacity="0.3">
    <polygon points="0,20 10,0 20,20 10,15"/>
    <polygon points="320,400 330,380 340,400 330,395"/>
  </g>
  
  <!-- Title -->
  <g transform="translate(200, 30)" text-anchor="middle" fill="white" font-family="Playfair Display, serif">
    <text y="0" font-size="24" font-weight="500">Elegant Growth</text>
  </g>
  
  <!-- Subtitle -->
  <g transform="translate(200, 480)" text-anchor="middle" fill="white" font-family="Montserrat" opacity="0.8">
    <text y="0" font-size="12" font-style="italic">The art of becoming</text>
  </g>
</svg>